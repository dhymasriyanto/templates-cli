    "bluebird": "^3.7.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "make-runnable": "^1.3.10",
    "multer": "^1.4.3",
    "pg-monitor": "^1.4.1",
    "pg-promise": "^10.11.1"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.36",
    "@types/express": "4.17.13",
    "@types/node": "16.10.2",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "typescript": "4.4.3"
  }
