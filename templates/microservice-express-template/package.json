{
  "name": "service-template",
  "version": "1.0.0",
  "description": "A template for ur service.",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon --trace-warnings --experimental-json-modules server.mjs",
